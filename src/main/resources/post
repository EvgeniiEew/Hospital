         http://localhost:8090/credanchials
          @GetMapping("/credanchials")

        class UsersController

        http://localhost:8090/users
         @GetMapping("/users")


SELECT credentials.firstname ,credentials.lastname , users.position,
doctor_ditales.name
FROM credentials, doctor_ditales , users
WHERE  users.id = doctor_ditales.doctorid and credentials.id = users.credential_id